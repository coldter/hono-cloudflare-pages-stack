{
  "name": "hono-weather",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "$( cat secrets.json | jq -r 'keys[] as $k | \"export \\($k)=\\(.[$k])\"') && wrangler pages dev --local --compatibility-date=2023-08-01 --binding OPEN_WEATHER_MAP_API=$OPEN_WEATHER_MAP_API OPEN_WEATHER_MAP_SEARCH_API=$OPEN_WEATHER_MAP_SEARCH_API -- pnpm vite:dev",
    "vite:dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "tsc:check": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.1.8",
    "framer-motion": "^10.16.1",
    "hono": "3.5.4",
    "js-cookie": "^3.0.5",
    "react": "^18.2.0",
    "react-debounce-input": "^3.3.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230821.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/js-cookie": "^3.0.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.0",
    "prettier": "^3.0.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-static-copy": "^0.17.0",
    "wrangler": "^3.6.0"
  }
}
